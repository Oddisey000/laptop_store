{
  "name": "laptop_store",
  "version": "1.0.0",
  "description": "A small Node.js project, laptop selling website",
  "main": "index.js",
  "scripts": {
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" dev/css/style_dev.css -o dev/css/style_concat_prefixed.css",
    "compress:css": "node-sass dev/css/style_concat_prefixed.css assets/css/style.css --output-style compressed",
    "concat:css": "concat -o dev/css/style_concat.css dev/css/fonts/nunito_sans.css dev/css/style_dev.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oddisey000/laptop_store.git"
  },
  "keywords": [
    "laptop",
    "store"
  ],
  "author": "Vitalii Pertsovych",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Oddisey000/laptop_store/issues"
  },
  "homepage": "https://github.com/Oddisey000/laptop_store#readme",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {}
}
